{
  "name": "fastify-status",
  "version": "1.1.1",
  "main": "lib/index.js",
  "config": {
    "releaseBranch": "master"
  },
  "scripts": {
    "changelog": "changelog -u https://gitlab.com/m03geek/server-status",
    "check-branch": "if test \"$(git rev-parse --abbrev-ref HEAD)\" = \"$npm_package_config_releaseBranch\" ; then exit 0 ; else  echo \"Not on '$npm_package_config_releaseBranch' branch\" && exit1 ; fi",
    "lint": "eslint .",
    "postversion": "git push && git push --tags",
    "preversion": "npm run check-branch && npm run lint && npm run unit",
    "test": "npm audit && npm run lint && npm run unit",
    "unit": "exit 0",
    "version": "changelog && git add -u",
    "pub": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkeLLLa/fastify-status.git"
  },
  "keywords": [
    "fastify",
    "status",
    "plugin",
    "fastify-plugin",
    "server-status"
  ],
  "author": "m03geek",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/m03geek/fastify-status/issues"
  },
  "homepage": "https://gitlab.com/m03geek/fastify-status#README",
  "dependencies": {
    "@m03geek/server-status": "^1.1.1",
    "fastify-plugin": "^1.2.1"
  },
  "directories": {
    "test": "__tests__",
    "lib": "lib"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.ts"
  ],
  "private": false,
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.1",
    "eslint": "^5.6.0",
    "eslint-config-google": "^0.10.0",
    "fastify": "^1.11.2",
    "generate-changelog": "^1.7.1",
    "jest": "^23.6.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "description": "Returns common info about fastify server. Could be used for healthchecks and status monitoring."
}
